_SECTION_BEGIN("ROC");
P = ParamField( "Price field" );
periods = Param("Periods", 15, 1, 200, 1 );
Plot( ROC( P, periods), _DEFAULT_NAME(), ParamColor( "Color", colorCycle ), ParamStyle("Style")  );
_SECTION_END();

_SECTION_BEGIN("RSI");
